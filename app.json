{
  "expo": {
    "name": "spotify-app",
    "slug": "spotify-app",
    "scheme": "siftify", // Changed from "syfty" to "siftify"
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/syfty-logo-final.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/syfty-logo-final.png",
      "resizeMode": "contain",
      "backgroundColor": "#545454"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.spotifyapp",
      "backgroundColor": "#545454",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/syfty-logo-final.png",
        "backgroundColor": "#545454"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.spotifyapp"
    },
    "web": {
      "favicon": "./assets/syfty-logo-final.png"
    },
    "extra": {
      "eas": {
        "projectId": "bffd0420-bba9-46e5-a45c-fd33a44bc2d3"
      }
    },
    "owner": "alex_vr"
  }
}
